{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "indent": ["error", "tab"],
    "quotes": ["error", "single"],
    "semi": ["error", "never"]
  },
  "ignorePatterns": ["migrations/**/*.js", "models/**/*.js"]
}
